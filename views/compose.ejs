<%- include('partials/header'); -%>

<h1>Compose</h1>
<form method="post" action="/compose">
    <div class="form-group">
        <label>Title</label><br>
        <input type="text" class="form-control" name="posttitle" aria-label="Sizing example input" aria-describedby="inputGroup-sizing-default">
        <label>Post</label><br>
        <textarea class="form-control" name="content" aria-label="With textarea" rows="8" ></textarea>
        
    </div>
    <button type="submit" class="btn btn-primary">Publish</button>
</form>


